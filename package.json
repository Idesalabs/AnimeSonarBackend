    "apollo-server-micro": "^2.9.16",
    "date-fns": "^2.15.0",
    "graphql": "^14.6.0",
    "graphql-import": "^1.0.0-beta.2",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
